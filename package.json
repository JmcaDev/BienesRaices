{
  "name": "bienesraices",
  "version": "1.0.0",
  "description": "Pagina Web de Bienes Raices con NodeJS",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon ./index.js",
    "css": "postcss ./public/css/tailwind.css -o ./public/css/app.css --watch",
    "js": "webpack --watch",
    "dev": "concurrently \"npm run css\" \"npm run js\" ",
    "db:importar": "node ./seed/seeder.js -i",
    "db:eliminar": "node ./seed/seeder.js -e"
  },
  "keywords": [
    "NodeJS",
    "Pug",
    "Sequelize",
    "Express",
    "MySql",
    "Tailwind"
  ],
  "author": "Jos√© Castillo @JmcaDev",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "dropzone": "^5.9.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.1.0",
    "nodemailer": "^6.9.1",
    "pug": "^3.0.2",
    "sequelize": "^6.28.0",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.2.4"
  }
}
